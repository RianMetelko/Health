ing_table name="prevnextnosubmit1110100111"><string id="prev">PREV</string><string id="next">NEXT</string><string id="submit">SUBMIT</string><string id="outline">Menu</string><string id="resources">Resources</string><string id="glossary">Glossary</string><string id="transcript">Notes</string><string id="clear">Clear and return to Menu</string><string id="search_results">Search Results</string><string id="filter">Filter</string><string id="slide_text_chk">Slide Text</string><string id="transcript_chk">Notes</string><string id="search_in">Search in:</string><string id="check_include">Check to include</string><string id="search">Search...</string><string id="terms">Terms</string><string id="definition">Definition</string><string id="acc_definition">definition</string><string id="acc_resources">resources</string><string id="acc_search_input">search</string><string id="acc_pause">pause</string><string id="acc_play">play</string><string id="acc_replay">replay</string><string id="acc_submit">submit</string><string id="acc_next">next</string><string id="acc_previous">previous</string><string id="acc_volume">volume</string></string_table></string_tables><sounds enabled="false" /><nav_data><outline><links><slidelink slideid="_player.6kfSYTUBbxK" displaytext="Health &amp; Saftey at Work" expand="true"><links><slidelink slideid="_player.6kfSYTUBbxK.665OriJfir6" displaytext="Title Slide" expand="true" /><slidelink slideid="_player.6kfSYTUBbxK.6X5zOyercxV" displaytext="Untitled Slide" expand="true" /><slidelink slideid="_player.6kfSYTUBbxK.6nthS8O1pVo" displaytext="Untitled Slide" expand="true" /><slidelink slideid="_player.6kfSYTUBbxK.6cQPvjqOETq" displaytext="Untitled Slide" expand="true" /><slidelink slideid="_player.6kfSYTUBbxK.653asHK4qw5" displaytext="Untitled Slide" expand="true" /><slidelink slideid="_player.6kfSYTUBbxK.5j4Y2xXySGT" displaytext="Tabs Interaction" expand="true" /><slidelink slideid="_player.6kfSYTUBbxK.6o9OpLlEpLe" displaytext="Untitled Slide" expand="true" /><slidelink slideid="_player.6kfSYTUBbxK.6oZ0uerfEeN" displaytext="Untitled Slide" expand="true" /><slidelink slideid="_player.6kfSYTUBbxK.648Tnxgm5Jf" displaytext="The Occupational Health and Safety Act and Regulations tell everyone from&#xA; the employer to the newest worker how to make the workplace safer." expand="true" /><slidelink slideid="_player.6kfSYTUBbxK.5lKe0P9UEVp" displaytext="It's against the law for my employer to fire or &#xA;punish me for doing what the OHSA says." expand="true" /></links></slidelink><slidelink slideid="_player.6gOLTSAVyZT" displaytext="Get in the know" expand="true"><links><slidelink slideid="_player.6gOLTSAVyZT.6mBK0Rgzj7U" displaytext="Untitled Slide" expand="true" /><slidelink slideid="_player.6gOLTSAVyZT.5p20gy0Y1tp" displaytext="Untitled Slide" expand="true" /><slidelink slideid="_player.6gOLTSAVyZT.5XH97hQ1gAm" displaytext="Untitled Slide" expand="true" /><slidelink slideid="_player.6gOLTSAVyZT.6pfORxzSb11" displaytext="Untitled Slide" expand="true" /><slidelink slideid="_player.6gOLTSAVyZT.6a1w3UUu6zX" displaytext="The Occupational Health and Safety Act says that you have the right to know about hazards&#xA; in your workplace?" expand="true" /><slidelink slideid="_player.6gOLTSAVyZT.6GkLBQ2m4Th" displaytext="If you see a hazard while you're working, you should report it to your supervisor or employer &#xA;right away?" expand="true" /></links></slidelink><slidelink slideid="_player.69CxTH217Fc" displaytext="Get Involved" expand="true"><links><slidelink slideid="_player.69CxTH217Fc.5kCzNKYzsVr" displaytext="Untitled Slide" expand="true" /><slidelink slideid="_player.69CxTH217Fc.5kYkFQUpkGW" displaytext="Untitled Slide" expand="true" /><slidelink slideid="_player.69CxTH217Fc.6kpTjBVdNXm" displaytext="Untitled Slide" expand="true" /><slidelink slideid="_player.69CxTH217Fc.6ecTa7PuuOD" displaytext="Untitled Slide" expand="true" /><slidelink slideid="_player.69CxTH217Fc.5iNH8pYQF9V" displaytext="It's important that you know the safe way to do your job. You should share what you know. &#xA;If you see a hazard, you must report it to your supervisor or employer?" expand="true" /><slidelink slideid="_player.69CxTH217Fc.6Bo30s3ks4y" displaytext="Your health and safety representative or joint health and safety committee can help you with&#xA; any concerns that you might have about working safely?" expand="true" /><slidelink slideid="_player.69CxTH217Fc.6SnX1bTBRay" displaytext="It's OK to take safety shortcuts to get the work done faster and on time?" expand="true" /></links></slidelink><slidelink slideid="_player.6JPib8HeFX1" displaytext="Health &amp; Saftey at Work" expand="true"><links><slidelink slideid="_player.6JPib8HeFX1.6pkGPQ5Yhaw" displaytext="Untitled Slide" expand="true" /><slidelink slideid="_player.6JPib8HeFX1.6OjxU4cu1Zo" displaytext="Untitled Slide" expand="true" /><slidelink slideid="_player.6JPib8HeFX1.6U8fz8t4aWU" displaytext="Untitled Slide" expand="true" /><slidelink slideid="_player.6JPib8HeFX1.6GzXzr1t3E6" displaytext="If you have reason to believe the equipment you are using might hurt you or someone &#xA;you work with, you have the legal right to refuse the work?" expand="true" /><slidelink slideid="_player.6JPib8HeFX1.69TTCQempme" displaytext="It's against the law for your employer or your supervisor to fire or punish you for doing what the &#xA;OHSA expects you to do, or for asking them to do what the OHSA expects them to do?" expand="true" /></links></slidelink><slidelink slideid="_player.5rIDluG8pnY" displaytext="What's Next" expand="true"><links><slidelink slideid="_player.5rIDluG8pnY.6kt2h55sH45" displaytext="Untitled Slide" expand="true" /></links></slidelink></links></outline></nav_data><resource_data description="&lt;p align='left'&gt;&lt;font face='Articulate' size='11' color='#444444'&gt;Here are some useful links and documents:&lt;/font&gt;&lt;/p&gt;"><resources><resource url="story_content/external_files/Ontario Heath &amp; Safety Worker Workbook.pdf" image="story_content/6SWjMKyTEHV.png" title="Ontario Heath &amp; Safety Worker Workbook" /><resource url="story_content/external_files/Ontario Health &amp; Safety Resources.pdf" image="story_content/5qyaTSnaoQ1.png" title="Ontario Health &amp; Safety Resources" /></resources></resource_data><transcript_data /><glossary_data /></bwFrame>